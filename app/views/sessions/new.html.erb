<% if @user.errors.any? %>
    <% @user.errors.full_messages.each do |error| %>
        <h4><%= error %></h4>
    <% end %>
<% end %>

<%= form_with url: '/login', local: true do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %><br><br>
    
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>


    
    <%= f.submit "Login" %>
<% end %>