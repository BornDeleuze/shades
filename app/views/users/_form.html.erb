<%= form_with model: @user, local: true do |f| %>
    <%= f.label :name%>
    <%= f.text_field :name %>
    <br>
    <%= f.label :email%>
    <%= f.email_field :email,required:true %>
    <br>
    <%= f.label :password%>
    <%= f.password_field :password,required:true %>
    <%= f.submit %>
<%end%>